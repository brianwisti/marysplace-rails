<% content_for :title do -%>
  Checkins for <%= @client.current_alias %>
<% end -%>

<% content_for :context_navigation do -%>
  <%= render 'context_navigation', client: @client %>
<% end -%>

<%= render @client %>

<%= paginate @checkins %>

<table class="table entries">
  <thead>
    <tr>
      <th>Checkin At</th>
      <th>Client</th>
      <th>Checked In By</th>
    </tr>
  </thead>
  <tbody>
    <%= render @checkins %>
  </tbody>
</table>

<%= paginate @checkins %>
