= content_for(:title) do
  Checkin Report for
  = @time_range

= content_for(:context_navigation) do
  = render 'context_navigation'

.container
  =# render 'annual_report_links', year: @year
  =# render 'monthly_report_links', span: @span
  = render 'daily_report_links', span: @span

%p
  %span.checkin-count= @total_checkins
  = "checkin".pluralize(@total_checkins)

%table.table.entries
  %thead
    %tr
      %th Time
      %th Client
      %th Location
      %th Checked In By
      %th Notes
  %tbody
    = render @checkins
